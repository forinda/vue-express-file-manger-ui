{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8080",
      "changeOrigin": true,
      "rewrite": "(path) => path.replace(/^/api/, '')"
    }
  },
  "dependencies": {
    "axios": "^1.3.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}
